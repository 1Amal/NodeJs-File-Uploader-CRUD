// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt DateTime
}

model Users {
  id        Int    @id @unique
  user_name String
  password  String
}

model FileDetails {
  id          Int    @id @unique
  file_url    String
  file_size   String
  folder_name String
}
